  "license": "MIT",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev": "concurrently \"npm start\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "loki test",
    "test:ui:ok": "loki approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "reg-cli ./.loki/current ./.loki/reference ./.loki/difference -R ./.loki/report.html -J ./.loki/report.json",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "analyze:prod": "webpack --env mode=production analyze=true",
    "analyze:dev": "webpack serve --env mode=development analyze=true",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "prepare": "husky install"
  },
